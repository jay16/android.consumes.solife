        <activity android:name="us.solife.consumes.TabUser" >
        </activity>
        <activity android:name="us.solife.consumes.TabAbout" >
        </activity>
        <activity android:name="us.solife.consumes.MainTabActivity" >
        </activity>
        <activity android:name="us.solife.consumes.Login" >
